
{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [{
    "matches": ["https://www.ica.se/*"],
    "js": ["content.js"]
  }]
}